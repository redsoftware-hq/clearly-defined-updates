{
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": "prettier",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": 1
  }
}
